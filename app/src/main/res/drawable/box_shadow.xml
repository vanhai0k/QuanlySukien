<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#DF8639" />
            <corners android:radius="28dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#DF8639"
                android:endColor="#DF8639"
            android:angle="90" />
            <corners android:radius="28dp" />
        </shape>
    </item>
</layer-list>
